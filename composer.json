{
    "name": "erjantj/game-of-three",
    "description": "Game of three.",
    "keywords": ["game", "three"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.0",
        "fzaninotto/faker": "^1.8"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "server": ["php server.php"],
        "client": ["php client.php"],
        "test": ["./vendor/bin/phpunit"]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "process-timeout": 0
    }
}
