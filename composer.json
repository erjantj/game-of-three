{
    "name": "erjantj/game-of-three",
    "description": "Game of three.",
    "keywords": ["game", "three"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "fzaninotto/faker": "^1.8"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "start": ["php -S 127.0.0.1:9000 -t public"],
        "test": ["./vendor/bin/phpunit"],
        "documentation": [
            "./vendor/bin/swagger app/ --output public/docs/swagger.json"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "process-timeout": 0
    }
}
